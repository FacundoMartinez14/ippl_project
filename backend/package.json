{
	"name": "ippl-backend",
	"version": "1.0.0",
	"description": "Backend para el sistema administrativo del IPPL",
	"main": "src/index.js",
	"scripts": {
		"start": "node src/index.js",
		"db:create": "sequelize-cli --config config/config.js db:create",
		"db:migrate": "sequelize-cli --config config/config.js db:migrate",
		"db:seed:all": "sequelize-cli --config config/config.js db:seed:all",
		"dev": "nodemon src/index.js",
		"setup": "node src/setup.js",
		"init-users": "node src/scripts/initUsers.js",
		"db:reset": "sequelize-cli --config config/config.js db:drop && npm run db:create && npm run db:migrate && npm run db:seed:all"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^8.15.1",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.14.3",
		"pg": "^8.16.3",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.37.7",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"nodemon": "^2.0.22",
		"sequelize-cli": "^6.6.3"
	}
}
